_*/_jsx(\"br\",{}),/*#__PURE__*/_jsx(\"h4\",{}),/*#__PURE__*/_jsx(\"p\",{}),/*#__PURE__*/_jsx(\"br\",{}),/*#__PURE__*/_jsx(\"h4\",{}),/*#__PURE__*/_jsx(\"p\",{}),/*#__PURE__*/_jsx(\"br\",{})]})]});}","map":{"version":3,"names":["React","useContext","Texts","Context","AboutMembership","lang","texts","aboutMembership","paddingInline","padding"],"sources":["C:/Users/D2105/Documents/Projects/Front-end/React/association/src/pages/AboutMembership.js"],"sourcesContent":["import React, { useContext } from 'react'\r\nimport Texts from '../Texts';\r\nimport { Context } from '../Context';\r\n\r\nexport default function AboutMembership() {\r\n\r\n  const { lang } = useContext(Context);\r\n  const texts = Texts[lang];\r\n\r\n  return (\r\n    <>\r\n      <div className='page-title'>\r\n        <h1>{texts.aboutMembership}</h1>\r\n      </div>\r\n      <div className='content' style={{ paddingInline: '300px' }}>\r\n        <h4>أنواع العضوية</h4>\r\n        <p>\r\n          <ul style={{ padding: '40px' }}>\r\n            <li>\r\n              <h4>عضو عامل:</h4>\r\n              <p>هو العضو الذي اشترك في تأسيس الجمعية، أو التحق بها بعد قيامها وقبل مجلس الإدارة عضويته، وكان من المتخصصين أو المهتمين أو الممارسين لتخصص الجمعية، ويحق للعضو حضور اجتماعات الجمعية العمومية والتصويت على قراراتها، ‌والترشح لعضوية مجلس الإدارة، وذلك بعد مدةً لا تقل عن ستة أشهر من تاريخ التحاقه بالجمعية</p>\r\n              <br />\r\n            </li>\r\n            <li>\r\n              <h4>عضو شرفي:</h4>\r\n              <p>يكون عضوًا شرفيًا في الجمعية من ترى الجمعية العمومية منحه عضوية شرفية بمجلس الإدارة نظير تميزه في مجال عمل الجمعية.</p>\r\n              <br />\r\n            </li>\r\n            <li>\r\n              <h4>عضو فخري:</h4>\r\n              <p>يكون عضوًا فخريًا في الجمعية من ترى الجمعية العمومية منحه عضوية فخرية فيها نظير مساهمته المادية أو المعنوية للجمعية.</p>\r\n              <br />\r\n            </li>\r\n            <li>\r\n              <h4>عضو منتسب:</h4>\r\n              <p>يكون العضو منتسبًا في الجمعية إذا تقدم بطلب عضوية عامل للجمعية وظهر عدم انطباق أحد شروط العضوية العاملة عليه وصدر قرار من مجلس الإدارة بقبوله عضواً منتسباً، ويحق للعضو الاطلاع على مستندات الجمعية والاشتراك في أنشطتها أو تقدم بطلب العضوية منتسبًا ولا يحق للعضو حضور اجتماعات الجمعية العمومية ولا الترشح لعضوية مجلس الإدارة.</p>\r\n              <br />\r\n            </li>\r\n          </ul>\r\n        </p>\r\n        <br />\r\n\r\n        <h4>شروط العضوية:</h4>\r\n        <p>\r\n          <ul style={{ padding: '40px' }}>\r\n            <li>‌أن يكون سعودي الجنسية.</li>\r\n            <li>‌ألا يقل عمره عن ثمانية عشر عاما.</li>\r\n            <li>‌ان يكون كامل الأھلیة المعتبرة شرعا.</li>\r\n            <li>‌ان لا يكون قد صدر بحقة حكم نھائي بإدانته في جريمة مخلة بالشرف او بالأمانة مالم يكن قد رد إليه اعتباره</li>\r\n            <li>‌ان يكون قد سدد الحد الأدنى من الاشتراك السنوي</li>\r\n            <li>‌التعاون مع الجمعية ومنسوبيها لتحقيق أهدافها.</li>\r\n            <li>‌عدم القيام بأي أمر من شأنه أن يلحق ضرراً بالجمعية.</li>\r\n            <li>‌الالتزام بقرارات الجمعية العمومية.</li>\r\n          </ul>\r\n        </p>\r\n        <br />\r\n\r\n        <h4>فقدان العضوية</h4>\r\n        <p>يفقد العضو عضويته في الحالات التالية:\r\n          <ul style={{ paddingInline: '40px' }}>\r\n            <li>الانسحاب من الجمعية بطلب كتابي.</li>\r\n            <li>الوفاة.</li>\r\n            <li>إذا فقد شرطاً من شروط العضوية.</li>\r\n            <li>إذا صدر قرار من الجمعية العمومية بسحب العضوية في الحالات الآتية:</li>\r\n            <li>إذا أقدم العضو على تصرف من شأنه أن يلحق ضرراً مادياً أو أدبياً بالجمعية.</li>\r\n            <li>إذا أقدم العضو باستغلال عضويته في الجمعية لغرض شخصي</li>\r\n            <li>إذا تأخر العضو عن أداء الاشتراك عن موعد استحقاقه قبل نهاية السنة المالية</li>\r\n          </ul>\r\n          ويجب على مجلس الإدارة في الحالات رقم (3) و(4) و(5) إبلاغ من زالت عضويته خطياً بزوال عضويته وحقه بالاعتراض<br/>\r\n          كما يجوز للعضو بعد انتفاء سبب زوال العضوية أن يقدم طلباً إلى مجلس الإدارة لرد العضوية إليه، وعلى المجلس أن يبت في الطلب بقرارٍ مسبب ويبلغه إلى العضو\r\n        </p>\r\n        <br />\r\n        <h4></h4>\r\n        <p></p>\r\n        <br />\r\n        <h4></h4>\r\n        <p></p>\r\n        <br />\r\n        <h4></h4>\r\n        <p></p>\r\n        <br />\r\n        <h4></h4>\r\n        <p></p>\r\n        <br />\r\n      </div>\r\n    </>\r\n  )\r\n}"],"mappings":"AAAA,MAAOA,MAAK,EAAIC,UAAU,KAAQ,OAAO,CACzC,MAAOC,MAAK,KAAM,UAAU,CAC5B,OAASC,OAAO,KAAQ,YAAY,CAAC,6IAErC,cAAe,SAASC,gBAAe,EAAG,CAExC,gBAAiBH,UAAU,CAACE,OAAO,CAAC,CAA5BE,IAAI,aAAJA,IAAI,CACZ,GAAMC,MAAK,CAAGJ,KAAK,CAACG,IAAI,CAAC,CAEzB,mBACE,wCACE,YAAK,SAAS,CAAC,YAAY,uBACzB,oBAAKC,KAAK,CAACC,eAAe,EAAM,EAC5B,cACN,aAAK,SAAS,CAAC,SAAS,CAAC,KAAK,CAAE,CAAEC,aAAa,CAAE,OAAQ,CAAE,wBACzD,iGAAsB,cACtB,gCACE,YAAI,KAAK,CAAE,CAAEC,OAAO,CAAE,MAAO,CAAE,wBAC7B,mCACE,oEAAkB,cAClB,oiDAAkT,cAClT,aAAM,GACH,cACL,mCACE,oEAAkB,cAClB,qmBAA0H,cAC1H,aAAM,GACH,cACL,mCACE,oEAAkB,cAClB,gnBAA2H,cAC3H,aAAM,GACH,cACL,mCACE,0EAAmB,cACnB,0pDAAyU,cACzU,aAAM,GACH,GACF,EACH,cACJ,aAAM,cAEN,4FAAsB,cACtB,gCACE,YAAI,KAAK,CAAE,CAAEA,OAAO,CAAE,MAAO,CAAE,wBAC7B,8IAAgC,cAChC,2LAA0C,cAC1C,kNAA6C,cAC7C,whBAA+G,cAC/G,oQAAuD,cACvD,wQAAsD,cACtD,wRAA4D,cAC5D,sNAA4C,GACzC,EACH,cACJ,aAAM,cAEN,iGAAsB,cACtB,qOACE,YAAI,KAAK,CAAE,CAAED,aAAa,CAAE,MAAO,CAAE,wBACnC,yLAAwC,cACxC,6DAAgB,cAChB,8KAAuC,cACvC,iWAAyE,cACzE,kYAAiF,cACjF,kSAA4D,cAC5D,4YAAiF,GAC9E,ggBACoG,aAAK,uvBAE5G,cACJ,aAAM,cACN,aAAS,cACT,YAAO,cACP,aAAM,cACN,aAAS,cACT,YAAO,cACP,aAAM,cACN,aAAS,cACT,YAAO,cACP,aAAM,cACN,aAAS,cACT,YAAO,cACP,aAAM,GACF,GACL,CAEP"},"metadata":{},"sourceType":"module","externalDependencies":[]}